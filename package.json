{
  "name": "iss-elements",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:element": "ng build --prod --output-hashing=none --single-bundle true --keep-polyfills",
    "bump-version": "cd src/assets/ && npm version patch",
    "copy-assets": "cp src/assets/package.json dist/issElements/package.json",
    "npm-pack": "cd dist/issElements && npm pack",
    "build:package": "npm run build:element && npm run bump-version && npm run copy-assets && npm run npm-pack",
    "publish": "cd dist/issElements && npm publish iss-elements*.tgz"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~11.0.5",
    "@angular/elements": "^7.2.10",
    "@angular/fire": "^5.1.2",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@swimlane/ngx-charts": "^10.1.0",
    "@webcomponents/custom-elements": "^1.2.2",
    "core-js": "^2.5.4",
    "document-register-element": "^1.7.2",
    "firebase": "^5.9.3",
    "ngx-build-plus": "^7.7.6",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.6",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
